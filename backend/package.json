{
  "name": "erp-system-backend",
  "version": "1.0.0",
  "description": "Backend for ERP System",
  "main": "index.js",
  "type": "commonjs",
  "scripts": {
    "start": "node index.js",
    "dev": "nodemon index.js",
    "test": "echo \"Error: no test specified\" && exit 1",
    "seed": "npx sequelize-cli db:seed:all",
    "seed:undo": "npx sequelize-cli db:seed:undo:all",
    "seed:production": "npx sequelize-cli db:seed --seed 20250101000000-production-seed.js",
    "seed:development": "node scripts/seed-all.js",
    "seed:undo:production": "npx sequelize-cli db:seed:undo --seed 20250101000000-production-seed.js",
    "seed:undo:development": "npx sequelize-cli db:seed:undo --seed 20250102000004-development-directory-records.js && npx sequelize-cli db:seed:undo --seed 20250102000003-development-directories.js && npx sequelize-cli db:seed:undo --seed 20250102000002-development-company-modules.js && npx sequelize-cli db:seed:undo --seed 20250102000001-development-users.js && npx sequelize-cli db:seed:undo --seed 20250102000000-development-companies.js && npx sequelize-cli db:seed:undo --seed 20250101000000-production-seed.js",
    "migrate": "npx sequelize-cli db:migrate",
    "migrate:reset": "npx sequelize-cli db:migrate:undo:all && npx sequelize-cli db:migrate",
    "pm2:start": "pm2 start ecosystem.config.js",
    "pm2:start:prod": "pm2 start ecosystem.config.js --env production",
    "pm2:stop": "pm2 stop ecosystem.config.js",
    "pm2:restart": "pm2 restart ecosystem.config.js",
    "pm2:reload": "pm2 reload ecosystem.config.js",
    "pm2:delete": "pm2 delete ecosystem.config.js",
    "pm2:logs": "pm2 logs",
    "pm2:monit": "pm2 monit",
    "pm2:status": "pm2 status"
  },
  "keywords": [
    "erp",
    "backend",
    "nodejs"
  ],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "axios": "^1.10.0",
    "bcrypt": "^6.0.0",
    "cors": "^2.8.5",
    "csv-parse": "^5.6.0",
    "dotenv": "^16.4.5",
    "exceljs": "^4.4.0",
    "express": "^4.18.3",
    "express-rate-limit": "^7.5.0",
    "faker": "^6.6.6",
    "jsonwebtoken": "^9.0.2",
    "morgan": "^1.10.0",
    "multer": "^2.0.1",
    "mysql2": "^3.14.1",
    "node-cache": "^5.1.2",
    "pg": "^8.16.0",
    "pg-hstore": "^2.3.4",
    "sequelize": "^6.37.7",
    "uuid": "^11.1.0",
    "xlsx": "^0.18.5"
  },
  "devDependencies": {
    "@faker-js/faker": "^9.8.0",
    "nodemon": "^3.1.10"
  }
}
